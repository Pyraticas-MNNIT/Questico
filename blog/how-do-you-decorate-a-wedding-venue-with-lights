<script>
jQuery(document).ready(function($) {
	$(".scroll").click(function (event){
		event.preventDefault();
    $('html,body').animate({scrollTop: $("#home").offset().top}, 500);
	});


	$(".scroll-btt").click(function (event){
		event.preventDefault();
    $('html, body').animate({scrollTop: $("#home").offset().top}, 500);
	});
});
</script>
	
